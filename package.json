{
  "name": "cherry",
  "version": "1.0.0",
  "description": "Code Analysis Tool",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "analyze-complexity": "node src/cli/analyze-complexity.js",
    "sonar:start": "docker-compose up -d sonarqube",
    "sonar:stop": "docker-compose down",
    "sonar": "sonar-scanner",
    "install-dependencies": "npm install && docker-compose pull",
    "test": "jest --coverage"
  },
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "eslint": "^9.22.0",
    "jest": "^29.5.0",
    "sonarqube-scanner": "^3.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.1",
    "axios": "^1.8.2",
    "body-parser": "^1.20.3",
    "chalk": "^4.1.2",
    "complexity-report": "^2.0.0-alpha",
    "dotenv": "^16.4.7",
    "esprima": "^4.0.1",
    "express": "^4.17.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0",
    "yargs": "^17.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "moduleNameMapper": {
      "^@logger$": "<rootDir>/src/logger"
    }
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "keywords": []
}
