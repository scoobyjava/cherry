# .env.example

# Basic Application Settings
DEBUG=True
HOST=0.0.0.0
PORT=8000

# API Keys and Tokens
OPENAI_API_KEY=your_openai_api_key_here
AIRTABLE_TOKEN=your_airtable_token_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
GCP_PROJECT_NUMBER=your_gcp_project_number_here
GEMINI_API_KEY=your_gemini_api_key_here
GROK_API_KEY=your_grok_api_key_here
RAILWAY_TOKEN=your_railway_token_here
VENICE_API_KEY=your_venice_api_key_here
ZILLIZ_API_KEY=your_zilliz_api_key_here

# Pinecone configuration
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment

# LLM Settings
DEFAULT_MODEL=gpt-4o
FALLBACK_MODEL=gpt-3.5-turbo

# Agent Settings
ENABLE_RESEARCH_AGENT=True
ENABLE_CREATIVE_AGENT=True
ENABLE_CODE_AGENT=True
ENABLE_ANALYTICS_AGENT=True

# Storage / Database Settings
DB_TYPE=memory
DB_URL=

# Task Settings
MAX_PENDING_TASKS=100
MAX_COMPLETED_TASKS=1000
TASK_TIMEOUT=300

# Logging Settings
LOG_LEVEL=INFO
LOG_FILE=cherry_ai.log
